<web-app>
    <servlet>
        <servlet-name>StoreMovieQuiz</servlet-name>
        <servlet-class>StoreMovieQuiz</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StoreMovieQuiz</servlet-name>
        <url-pattern>/storemoviequiz</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AccessMovieQuiz</servlet-name>
        <servlet-class>AccessMovieQuiz</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AccessMovieQuiz</servlet-name>
        <url-pattern>/accessmoviequiz</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EvaluationMovieQuiz</servlet-name>
        <servlet-class>EvaluationMovieQuiz</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EvaluationMovieQuiz</servlet-name>
        <url-pattern>/evaluationmoviequiz</url-pattern>
    </servlet-mapping>
</web-app>
